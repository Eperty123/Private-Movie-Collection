<AnchorPane prefHeight="500.0" prefWidth="400.0" stylesheets="@modena_dark.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.CONTROLLER.AddMovieController">
